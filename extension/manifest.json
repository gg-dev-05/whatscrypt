{
  "name": "Whatscrypt",
  "description": "Encrypt the encrypted",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [ "*://web.whatsapp.com/*" ],
      "js": ["encrypt.js", "functions.js", "logic.js"]
    }
  ],
  "host_permissions": [
    "https://whatscrypt.herokuapp.com/*"
  ],
  "icons": { 
          "128": "128.png" 
        },
  "permissions": ["storage"]
}